<project name = "Shoppingcart" default="run">
    <target name="compile">
        <mkdir dir="build"/>
        <javac srcdir = "src" destdir="build"/>
    </target>
    <target name = "run" depends="compile">
        <java classname="Shoppingcart" classpath="build"/>
    </target>
    <target name = "clean">
        <delete dir = "build"/>
        </target>
</project>